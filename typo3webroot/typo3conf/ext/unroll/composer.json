{
	"name": "instituteweb/unroll",
	"description": "Adds options to TYPO3's save buttons, to unroll them from dropdown, just display icons and changing the order.",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Armin Vieweg",
			"email": "armin@v.ieweg.de",
			"role": "Developer",
			"homepage": "http://v.ieweg.de"
		},
		{
			"name": "Markus Klein, Reelworx GmbH",
			"role": "Developer",
			"homepage": "https://reelworx.at/"
		}
	],
	"replace": {
		"unroll": "self.version",
		"typo3-ter/unroll": "self.version",
		"typo3-ter/rx-unrollsavebuttons": "*",
		"reelworx/rx-unrollsavebuttons": "*"
	},
	"require": {
		"typo3/cms-core": "7.6.2 - 8.7.99",
		"typo3/cms-setup": "*"
	},
	"autoload": {
		"psr-4": {
			"InstituteWeb\\Unroll\\": "Classes/"
		}
	}
}
